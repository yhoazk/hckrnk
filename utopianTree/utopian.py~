import sys


t = int(raw_input().strip())
for a0 in xrange(t):
    n = int(raw_input().strip())
    h = 1
    #comenzando con una altura de 1
    for c in xrange(n):
        print c,
        if (c % 2) == 1:
#            print "<-odd"
#            print "pre h: " + str(h)
            h = h + 1
#            print "post h: " + str(h)
        else:
#            print "<-even"
#            print "pre h: " + str(h)
            h = h * 2
#            print "post h: " + str(h)
    print str(h)


